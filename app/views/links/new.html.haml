.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", links_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_link_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Link
    .inner
      = form_for :link, :url => links_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}

